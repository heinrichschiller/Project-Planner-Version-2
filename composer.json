{
    "name": "heinrich/workflow",
    "description": "Rewrite code from Project-Planner version 1",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Heinrich Schiller",
            "email": "heinrich@heinrich-schiller.de"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "mustache/mustache": "2.13.0",
        "vlucas/phpdotenv": "5.2.*",
        "doctrine/orm": "2.7.*",
        "slim/slim": "4.*",
        "slim/psr7": "1.*",
        "php-di/php-di": "6.3.*",
        "cakephp/validation": "4.1.*"
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "psr-4": {
            "App\\": "src/",
            "Entities\\": "src/Entities/",
            "Helper\\": "src/Helper/",
            "Traits\\": "src/Traits/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev",
        "symfony/var-dumper": "9999999-dev",
        "phpstan/phpstan": "0.12.x-dev"
    }
}
