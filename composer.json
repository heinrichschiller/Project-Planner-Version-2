{
    "name": "heinrich/workflow",
    "description": "Rewrite code from Project-Planner version 1",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Heinrich Schiller",
            "email": "heinrich@heinrich-schiller.de"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "mustache/mustache": "2.13.0",
        "vlucas/phpdotenv": "5.1.0",
        "doctrine/orm": "2.7.3",
        "slim/slim": "4.*",
        "slim/psr7": "dev-master",
        "php-di/php-di": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "psr-4": {
            "App\\": "src/",
            "Entities\\": "src/Entities/",
            "Helper\\": "src/Helper/",
            "Traits\\": "src/Traits/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/var-dumper": "^5.1.2"
    }
}
